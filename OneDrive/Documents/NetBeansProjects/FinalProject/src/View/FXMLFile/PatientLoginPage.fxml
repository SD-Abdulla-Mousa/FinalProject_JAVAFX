                  <Label alignment="CENTER" layoutX="93.0" layoutY="26.0" prefHeight="35.0" prefWidth="194.0" text="Welcome" textFill="#1100faf8">
                     <font>
                        <Font name="Franklin Gothic Medium" size="29.0" />
                     </font></Label>
                  <TextField fx:id="patientusernameTf" layoutX="93.0" layoutY="136.0" prefHeight="43.0" prefWidth="194.0" promptText="User_name">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
                  <Button fx:id="submitButton" layoutX="59.0" layoutY="308.0" mnemonicParsing="false" onAction="#changeSceneToPatientDashboard" prefHeight="51.0" prefWidth="234.0" style="-fx-background-color: #9cfe05; -fx-background-radius: 30;" text="Submit">
                     <font>
                        <Font name="Ebrima" size="18.0" />
                     </font>
                  </Button>
                  <TextField fx:id="passwordTf" accessibleRole="PASSWORD_FIELD" layoutX="93.0" layoutY="221.0" prefHeight="43.0" prefWidth="194.0" promptText="Password">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
