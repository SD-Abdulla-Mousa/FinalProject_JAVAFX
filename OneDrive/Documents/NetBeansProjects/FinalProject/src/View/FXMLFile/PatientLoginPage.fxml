<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.PatientLoginPageController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="475.0" style="-fx-background-color: #1095d7e6;">
         <children>
            <AnchorPane layoutX="57.0" layoutY="56.0" prefHeight="420.0" prefWidth="351.0" style="-fx-background-color: #83d6ff;">
