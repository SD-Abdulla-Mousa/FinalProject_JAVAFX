            <ImageView fitHeight="195.0" fitWidth="226.0" layoutX="43.0" layoutY="112.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/6519207.png" />
               </image>
            </ImageView>
            <Button fx:id="regsterButton" layoutX="23.0" layoutY="330.0" mnemonicParsing="false" onAction="#changeSceneToRegisterationPage" prefHeight="51.0" prefWidth="234.0" style="-fx-background-color: #1cfe95; -fx-background-radius: 50;" text="Register HERE ...">
